<p> <h1>Board name: <%= @board.board_name %></h1></p>

  <table>
    <% JSON.parse(@board.board_data).each do |row| %>
      <tr>
        <% row.each do |cell| %>
          <td><%= cell %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
<p>Generated by: <%= @board.email %> on <%= @board.created_at.strftime("%B %d, %Y at %I:%M %p") %></p>

<%= link_to "Back to all generated boards", board_index_path %>
