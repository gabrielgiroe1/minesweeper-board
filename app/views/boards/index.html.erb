<% @boards.each do |board| %>
  <p> <h1>Board name: <%= board.board_name %></h1>
  <table>
    <% board.generate_board.each do |row| %>
      <tr>
        <% row.each do |cell| %>
          <td><%= cell %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
  <p>Generated by: <%= board.email %> on <%= board.created_at.strftime("%B %d, %Y at %I:%M %p") %></p>
  <hr >
<% end %>